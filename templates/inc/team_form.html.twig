{{ form_start(form) }}
  {{ form_errors(form) }}
  <div class="form-group">
    <label class="text-light">Komandos pavadinimas</label>
    {{ form_errors(form.name) }}
    {{ form_widget(form.name) }}
  </div>
  <div class="form-group">
    <label class="text-light">Šalis</label>
    {{ form_errors(form.country) }}
    {{ form_widget(form.country) }}
  </div>
  <div class="form-group">
    <label class="text-light">Lokacija</label>
    {{ form_errors(form.region) }}
    {{ form_widget(form.region) }}
  </div>
  <div class="form-group">
    <label class="text-light">Aikštė</label>
    {{ form_errors(form.venue) }}
    {{ form_widget(form.venue) }}
  </div>
  <div class="form-group">
    <label class="text-light">Kontaktai</label>
    {{ form_errors(form.contacts) }}
    {{ form_widget(form.contacts) }}
  </div>
  {{ form_widget(form._token) }}
  {{ form_widget(form.submit) }}
{{ form_end(form) }}