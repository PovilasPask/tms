{{ form_start(form) }}
  {{ form_errors(form) }}
  <div class="form-group">
    <label class="text-light">Žaidėjo vardas ir pavardė</label>
    {{ form_errors(form.name) }}
    {{ form_widget(form.name) }}
  </div>
  <div class="form-group">
    <label class="text-light">Gimimo data</label>
    {% if not dateOk %}
      <ul><li>Netinkama data.</li></ul>  
    {% endif %}
    <div class="form-row">
      <div class="col-md-6">
        {{ form_errors(form.year) }}
        {{ form_widget(form.year) }}
      </div>
      <div class="col-md-3">
        {{ form_errors(form.month) }}
        {{ form_widget(form.month) }}
      </div>
      <div class="col-md-3">
        {{ form_errors(form.day) }}
        {{ form_widget(form.day) }}
      </div>
    </div>
  </div>
  <div class="form-group">
    <label class="text-light">Numeris (nebūtinas)</label>
    {{ form_errors(form.number) }}
    {{ form_widget(form.number) }}
  </div>
  {{ form_widget(form._token) }}
  {{ form_widget(form.submit) }}
{{ form_end(form) }}